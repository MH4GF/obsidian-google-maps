{
  "name": "obsidian-google-maps",
  "version": "0.1.2",
  "description": "Sync your Google Maps saved places from Takeout export to your vault",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "all": "bun run typecheck && bun run format:check && bun run lint:check && bun run knip:check && bun run test",
    "typecheck": "tsc --noEmit",
    "format:check": "biome check .",
    "format:fix": "biome check . --write",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "knip:check": "knip",
    "knip:fix": "knip --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "license": "0-BSD",
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@eslint/js": "9.30.1",
    "@mh4gf/configs": "^0.5.0",
    "@mh4gf/eslint-config": "^0.6.12",
    "@types/node": "^16.18.126",
    "@vitest/coverage-v8": "^4.0.16",
    "esbuild": "0.25.5",
    "eslint": "^9.39.2",
    "eslint-plugin-obsidianmd": "0.1.9",
    "globals": "14.0.0",
    "jiti": "2.6.1",
    "knip": "^5.78.0",
    "lefthook": "^2.0.13",
    "tslib": "2.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "8.35.1",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "obsidian": "latest"
  }
}
